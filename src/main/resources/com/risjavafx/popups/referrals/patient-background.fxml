                                       <font>
                                          <Font size="24.0" />
                                       </font>
                                    </Label>
                                    <Button alignment="CENTER" contentDisplay="CENTER" layoutX="274.0" layoutY="332.0" mnemonicParsing="false" onMouseClicked="#addListNameFoodMedicine" prefHeight="22.0" prefWidth="53.0" text="Add" />
