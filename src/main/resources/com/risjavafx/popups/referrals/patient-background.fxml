                                 </children>
                              </AnchorPane>
                           </content>
                        </Tab>
                        <Tab text="Previous Surgeries">
                           <content>
                              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <children>
                                    <VBox layoutX="88.0" layoutY="85.0" prefHeight="202.0" prefWidth="425.0">
