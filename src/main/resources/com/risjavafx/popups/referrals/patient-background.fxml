                                 <children>
                                    <VBox layoutX="88.0" layoutY="85.0" prefHeight="202.0" prefWidth="425.0">
                                       <children>
                                          <ListView fx:id="InstalledMedicalDevicesList" prefHeight="195.0" prefWidth="600.0">
